            android:layout_marginStart="16dp"
            app:layout_constraintStart_toEndOf="@+id/dlg_color15"
            app:layout_constraintTop_toTopOf="@+id/dlg_color15" />

        <com.example.timeplantest.weight.RoundCornerView
            android:id="@+id/dlg_color14"

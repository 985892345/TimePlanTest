            android:layout_marginTop="16dp"
            android:layout_marginEnd="24dp"
            app:layout_constraintEnd_toStartOf="@+id/dlg_default_inside_color"
            app:layout_constraintHorizontal_chainStyle="packed"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <com.example.timeplantest.weight.RoundCornerView
            android:id="@+id/dlg_default_inside_color"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/dlg_default_border_color"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/dlg_default_border_color"
            app:layout_constraintTop_toTopOf="@+id/dlg_default_border_color" />

        <com.example.timeplantest.weight.RoundCornerView
            android:id="@+id/dlg_color1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
